  "steps": [
    {
      "index": 1,
      "instructionsMarkdown": "200 ml heiße Gemüsebrühe zubereiten, Couscous darin einstreuen, umrühren und ziehen lassen.",
      "instructionsHTML": "<p>200 ml heiße Gemüsebrühe zubereiten, Couscous darin einstreuen, umrühren und ziehen lassen.</p>",
      "instructions": "200 ml heiße Gemüsebrühe zubereiten, Couscous darin einstreuen, umrühren und ziehen lassen.",

  "steps": [
    {
      "index": 1,
      "instructionsMarkdown": "Den Backofen auf 200 °C Ober-/Unterhitze vorheizen. Kürbiskerne mit dem Boden eines kleinen Topfs zerdrücken.",
      "instructionsHTML": "<p>Den Backofen auf 200 °C Ober-/Unterhitze vorheizen. Kürbiskerne mit dem Boden eines kleinen Topfs zerdrücken.</p>",
      "instructions": "Den Backofen auf 200 °C Ober-/Unterhitze vorheizen. Kürbiskerne mit dem Boden eines kleinen Topfs zerdrücken.",
      "timers": [],
      "images": [

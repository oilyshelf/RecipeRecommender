  "steps": [
    {
      "index": 1,
      "instructionsMarkdown": "Hähnchenfilets aus dem Kühlschrank nehmen. Schaschlik-Spieße in einem tiefen Teller in ein wenig Wasser einweichen.",
      "instructionsHTML": "<p>Hähnchenfilets aus dem Kühlschrank nehmen. Schaschlik-Spieße in einem tiefen Teller in ein wenig Wasser einweichen.</p>",
      "instructions": "Hähnchenfilets aus dem Kühlschrank nehmen. Schaschlik-Spieße in einem tiefen Teller in ein wenig Wasser einweichen.",
      "timers": [],

  "steps": [
    {
      "index": 1,
      "instructionsMarkdown": "Wenn Ihr Eure Kinder mit ins Kochgeschehen einbinden möchtet, lasst sie doch die Soße anrühren.",
      "instructionsHTML": "<p>Wenn Ihr Eure Kinder mit ins Kochgeschehen einbinden möchtet, lasst sie doch die Soße anrühren.</p>",
      "instructions": "Wenn Ihr Eure Kinder mit ins Kochgeschehen einbinden möchtet, lasst sie doch die Soße anrühren.",
      "timers": [],
      "images": [
